{
  "name": "drawgaiden-client",
  "version": "0.6.3",
  "private": true,
  "description": "Real time collaborative drawing canvas.",
  "main": "src/main.jsx",
  "scripts": {
    "test": "jest",
    "test:update": "jest -u",
    "build": "webpack --config=webpack.production.config.js",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base src/client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cschram/drawgaiden.git"
  },
  "author": "Corey Schram <coreyschram@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cschram/drawgaiden/issues"
  },
  "homepage": "https://github.com/cschram/drawgaiden#readme",
  "dependencies": {
    "@drawgaiden/easel": "^0.2.2",
    "@types/jest": "^20.0.7",
    "@types/lodash.throttle": "^4.1.2",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "@types/react-router": "^3.0.11",
    "@types/react-router-redux": "^4.0.46",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/reselect": "^2.2.0",
    "@types/socket.io-client": "^1.4.29",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "jest": "^20.0.4",
    "lodash.throttle": "^4.1.1",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.6.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.3",
    "simplify-js": "^1.2.1",
    "socket.io-client": "^2.0.1",
    "style-loader": "^0.16.1",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.1.0",
    "typescript": "^2.3.3",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/file.js",
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/style.js"
    }
  }
}
