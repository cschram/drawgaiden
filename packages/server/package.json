{
  "name": "drawgaiden-server",
  "version": "0.7.0",
  "private": true,
  "description": "Real time collaborative drawing canvas.",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && npm run watch",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Server\" -c \"green.bold,purple.bold\" \"npm run watch:ts\" \"npm run watch:server\"",
    "watch:ts": "tsc -w",
    "watch:server": "nodemon --inspect dist/socket/index.js",
    "build": "tsc",
    "test": "echo \"No server tests.\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cschram/drawgaiden.git"
  },
  "author": "Corey Schram <coreyschram@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cschram/drawgaiden/issues"
  },
  "homepage": "https://github.com/cschram/drawgaiden#readme",
  "dependencies": {
    "@drawgaiden/easel": "^0.2.2",
    "@types/cuid": "^1.3.0",
    "@types/hapi": "^16.1.9",
    "@types/minimist": "^1.2.0",
    "@types/rethinkdb": "^2.3.2",
    "@types/socket.io": "^1.4.29",
    "@types/winston": "^2.3.2",
    "canvas": "^1.6.5",
    "cuid": "^1.3.8",
    "hapi": "^16.5.2",
    "minimist": "^1.2.0",
    "rethinkdb": "^2.3.3",
    "simplify-js": "^1.2.1",
    "socket.io": "^2.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "drawgaiden-common": "^0.7.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.3.3"
  }
}
